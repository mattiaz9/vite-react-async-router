{
  "compilerOptions": {
    "target": "ESNext",
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "types": ["vite/client"],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noEmit": true,
    "jsx": "react",
    "typeRoots": ["typings"],
    "baseUrl": ".",
    "paths": {
      "vite-react-async-router": ["../../dist/index.d.js"]
    }
  },
  "include": ["src", "pages", "async-router.env.d.ts"],
  "exclude": ["build", "node_modules", "public"]
}
